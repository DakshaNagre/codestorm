pipeline
{
    agent any
    tools {
            nodejs 'NodeJs 13.11.0'
        }

    environment {
        registry = "docker_hub_account/codestorm2020"
        registryCredential = 'docker_credentials'
    }

    stages
    {
        stage('Build')
        {
            steps
            {
                    cd APIGateway/RESTGateway
                    docker build()
            }
        }
     }
}
