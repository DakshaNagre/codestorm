pipeline
{
    agent any
    tools {
            nodejs 'NodeJs 13.11.0'
        }

    stages
    {
        stage('Build')
        {
            steps
            {
                sh '''
                    cd APIGateway/RESTGateway
                    script
                    {
                        docker.build registry + ":$BUILD_NUMBER"
                    }
                '''
            }
        }
     }
}
